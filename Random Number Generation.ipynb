    "# Compute P(x>=40) from theoratical binomial distribution\n",
    "from scipy.stats import binom\n",
    "binom.pmf(40, 44, 0.64)+binom.pmf(41, 44, 0.64)+binom.pmf(42, 44, 0.64)+binom.pmf(43, 44, 0.64)+binom.pmf(44, 44, 0.64)"
   ]
